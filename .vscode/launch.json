  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run codygen",
      "runtimeExecutable": "codygen",
      "cwd": "${workspaceFolder}",
      "args": []
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/packages/codygen/src/lib/commands/extract.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
